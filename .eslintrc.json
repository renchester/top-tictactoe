{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "wrap-iife": ["error", "inside"],
    "func-names": ["error", "never"],
    "no-plusplus": "off"
  }
}
